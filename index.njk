<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Pogo Paste</title>
  <link rel="stylesheet" href="/style.css">
</head>
<body>
  <main>
    <h1>Pogo Paste</h1>

    <div id="toast" class="toast hidden"></div>

    <table id="symbolsTable">
      <tbody>
        {% for group in collections.all[0].data.symbols %}
            <tr><td colspan="2">
                <h2>{{ group.name }}</h2>
            </td></tr>
            {% for item in group.items %}
                <tr>
                <td class="{{item.className}}">{{ item.name }}</td>
                <td class="copy-btn-cell">
                    <input type="button" value="{{ item.symbol }}" class="copy-btn" />
                </td>
                </tr>
            {% endfor %}
        {% endfor %}
      </tbody>
    </table>
  </main>

  <script src="/script.js"></script>
</body>
</html>
